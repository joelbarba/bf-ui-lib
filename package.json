{
  "name": "bf-ui-lib",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 --no-live-reload",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --progress false --code-coverage --browsers ChromeHeadless --watch false",
    "lint": "ng lint",
    "lint:ci": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration ci",
    "e2e:webdriver:update": "webdriver-manager update --gecko=false --versions.chrome $(google-chrome --version | sed 's/Google Chrome //')",
    "build_lib": "ng build bf-ui-lib",
    "copy_md": "cp ./README.md ./dist/bf-ui-lib/",
    "copy_styles": "cp -r ./projects/bf-ui-lib/scss ./dist/bf-ui-lib/scss",
    "version_up": "cd ./projects/bf-ui-lib && npm version patch",
    "npm_pack": "cd dist/bf-ui-lib && npm pack",
    "pack_all": "npm run build_lib && npm run copy_md  &&  npm run copy_styles  &&  npm run npm_pack",
    "publish": "npm run version_up && npm run pack_all &&  npm publish ./dist/bf-ui-lib/bf-ui-lib-$(npm view ./projects/bf-ui-lib version).tgz",
    "build_doc": "ng build bf-ui-lib-sandbox --deployUrl=bf-ui-lib-doc/ --base-href '/bf-ui-lib-doc'"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^8.1.0",
    "@angular/common": "^8.1.0",
    "@angular/compiler": "^8.1.0",
    "@angular/core": "^8.1.0",
    "@angular/forms": "^8.1.0",
    "@angular/platform-browser": "^8.1.0",
    "@angular/platform-browser-dynamic": "^8.1.0",
    "@angular/router": "^8.1.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "@ngx-translate/core": "^11.0.1",
    "bf-icomoon": "^1.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "debug": "^4.1.1",
    "ng5-slider": "^1.2.4",
    "rxjs": "^6.3.3",
    "subsink": "^1.0.1",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular-devkit/build-ng-packagr": "~0.803.25",
    "@angular/cli": "^8.1.0",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "^8.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "html-dnd": "^1.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^5.7.1",
    "node-sass": "^4.13.0",
    "protractor": "^5.4.2",
    "ts-node": "~8.3.0",
    "tsickle": ">=0.34.0",
    "tslint": "~5.18.0",
    "tslint-jasmine-rules": "^1.6.0",
    "yargs": "^14.2.0"
  }
}
