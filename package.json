{
  "name": "bf-ui-lib",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 --no-live-reload",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --progress false --code-coverage --browsers ChromeHeadless --watch false",
    "test:ci:no-coverage": "ng test --progress false --browsers ChromeHeadless --watch false",
    "lint": "ng lint",
    "lint:ci": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration ci",
    "e2e:webdriver:update": "webdriver-manager update --gecko=false --versions.chrome $(google-chrome --version | sed 's/Google Chrome //')",
    "build_lib": "ng build bf-ui-lib",
    "copy_md": "cp ./README.md ./dist/bf-ui-lib/",
    "copy_styles": "cp -r ./projects/bf-ui-lib/scss ./dist/bf-ui-lib/scss",
    "version_up": "cd ./projects/bf-ui-lib && npm version patch",
    "version_bump:ci": "cd ./projects/bf-ui-lib && npm version $version_bump",
    "npm_pack": "cd dist/bf-ui-lib && npm pack",
    "pack_all": "npm run build_lib && npm run copy_md && npm run copy_styles && npm run npm_pack",
    "publish": "npm run version_up && npm run pack_all && npm publish ./dist/bf-ui-lib/bf-ui-lib-$(npm view ./projects/bf-ui-lib version).tgz",
    "prepare:lib": "npm run version_bump:ci && npm run pack_all",
    "build_doc": "ng build bf-ui-lib-sandbox --deployUrl=bf-ui-lib-doc/ --base-href '/bf-ui-lib-doc'"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/localize": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@ng-bootstrap/ng-bootstrap": "^14.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@popperjs/core": "^2.11.6",
    "bf-icomoon": "^1.0.0",
    "bootstrap": "^4.6.1",
    "core-js": "^3.21.1",
    "debug": "^4.3.4",
    "focus-visible": "^5.2.0",
    "rxjs": "^7.1.0",
    "subsink": "^1.0.2",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-devkit/core": "^15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.8",
    "@angular/compiler-cli": "^15.2.9",
    "@angular/language-service": "^15.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/jquery": "^3.5.14",
    "@types/node": "^14.18.12",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "html-dnd": "^1.2.1",
    "jasmine-core": "~3.8.0",
    "jasmine-reporters": "^2.4.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "^3.6.0",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^15.2.2",
    "protractor": "^7.0.0",
    "spectrum-colorpicker": "^1.8.1",
    "ts-node": "~8.10.2",
    "tslint-jasmine-rules": "^1.6.1",
    "typescript": "^4.8.4",
    "yargs": "^15.4.1"
  }
}
