{
  "name": "bf-ui-lib",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --poll=2000 --no-live-reload",
    "build": "ng build",
    "test": "ng test",
    "test:ci": "ng test --progress false --code-coverage --browsers ChromeHeadless --watch false",
    "test:ci:no-coverage": "ng test --progress false --browsers ChromeHeadless --watch false",
    "lint": "ng lint",
    "lint:ci": "ng lint",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --configuration ci",
    "e2e:webdriver:update": "webdriver-manager update --gecko=false --versions.chrome $(google-chrome --version | sed 's/Google Chrome //')",
    "build_lib": "ng build bf-ui-lib",
    "copy_md": "cp ./README.md ./dist/bf-ui-lib/",
    "copy_styles": "cp -r ./projects/bf-ui-lib/scss ./dist/bf-ui-lib/scss",
    "version_up": "cd ./projects/bf-ui-lib && npm version patch",
    "version_bump:ci": "cd ./projects/bf-ui-lib && npm version $version_bump",
    "npm_pack": "cd dist/bf-ui-lib && npm pack",
    "pack_all": "npm run build_lib && npm run copy_md && npm run copy_styles && npm run npm_pack",
    "publish": "npm run version_up && npm run pack_all && npm publish ./dist/bf-ui-lib/bf-ui-lib-$(npm view ./projects/bf-ui-lib version).tgz",
    "prepare:lib": "npm run version_bump:ci && npm run pack_all",
    "build_doc": "ng build bf-ui-lib-sandbox --deployUrl=bf-ui-lib-doc/ --base-href '/bf-ui-lib-doc'"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/localize": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ng-bootstrap/ng-bootstrap": "^15.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@popperjs/core": "^2.11.6",
    "bf-icomoon": "^1.0.0",
    "bootstrap": "^4.6.1",
    "debug": "^4.3.4",
    "focus-visible": "^5.2.0",
    "rxjs": "~7.8.0",
    "subsink": "^1.0.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.3",
    "@angular-devkit/core": "^16.0.3",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/language-service": "^16.2.0",
    "@types/jasmine": "~4.3.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/jquery": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "jasmine-core": "~4.6.0",
    "jasmine-reporters": "^2.5.2",
    "jasmine-spec-reporter": "~5.0.2",
    "jquery": "^3.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^16.0.1",
    "protractor": "^7.0.0",
    "puppeteer": "^20.5.0",
    "spectrum-colorpicker": "^1.8.1",
    "tslint-jasmine-rules": "^1.6.1",
    "typescript": "^5.0.4",
    "yargs": "^15.4.1"
  }
}
